alias . 'pwd'
alias .. 'cd ..'
alias ... 'cd ../..'
alias .... 'cd ../../..'
alias ..... 'cd ../../../..'
alias ..... 'cd ../../../../..'
alias cd.. 'cd ..'
alias rm 'rm -i'
alias cp 'cp -i'
alias mv 'mv -i'
alias l 'ls -lhaF --color=auto --show-control-chars'
alias ll 'ls -lhaF --color=auto --show-control-chars | less'
alias lt 'ls -lhatF --color=auto --show-control-chars'
alias llt 'ls -lhatF --color=auto --show-control-chars | less'
alias dir 'ls -alF --color=auto --show-control-chars'
alias c 'clear'
alias e 'exit'
alias tx '/bin/tar -xzf'
alias tc '/bin/tarr -czf'